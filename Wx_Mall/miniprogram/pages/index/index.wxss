.tabbar{
    width: 100%;
    height: 60px;
    display: flex;
    flex-direction: row;
    align-items: center;
    background: #fff;
    border-bottom: 1px solid #e5e5e5;
}
.tabbox{
    margin: 11px auto 0;
    height: 49px;
    flex: 1;
    align-items: center;
    box-sizing: border-box;
}
.curtab{
    border-bottom: 2px solid #f7545f;
}
.tabico{
    margin: 0 auto;
    width: 24px;
    height: 23px;
    position: relative;
}
.point-tip{
    position: absolute;
    top: 0;
    right: -1px;
    z-index: 2;
    width: 7.5px;
    height: 7.5px;
    border-radius: 50%;
    background: #ff3535;
}
